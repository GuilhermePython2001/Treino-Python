from math import sqrt,pow,floor
num1=float(input('Digite qualquer numero:'))
r=sqrt(num1)
p=pow(num1,2)
arb=floor(num1)
arc=ceil(num1)
print('A raiz deste numero é {} , seu resultado quando elevado ao quadrado é {}'
      ' \nSua aproximação para o menor inteiro é {} e sua aproximação para o maior inteiro é {}'.format(r,p,arb,arc))
